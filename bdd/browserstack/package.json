{
  "name": "buyacar_next_bdd",
  "version": "0.6.0",
  "description": "Buyacar Nextjs platform BDD tests",
  "scripts": {
    "test": "cucumber-js features/*/**.feature",
    "test:local": "TARGET_URL=http://localhost UI=1 cucumber-js features/*/**.feature",
    "test:tags": "cucumber-js --tags",
    "test:s": "cucumber-js features/forms/scc_form.feature --format progress",
    "test:p": "CONFIG_FILE=parallel ./scripts/cucumber-runner.js features/reservation.feature"
  },
